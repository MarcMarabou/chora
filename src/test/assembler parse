# Populate some registers 
add r1 = r0, 1
add r2 = r0, 2
add r3 = r0, 3
add r4 = r0, 4
add r5 = r0, 5
add r6 = r0, 6
add r7 = r0, 7
add r8 = r0, 8

# RegEx 1
add r1 = r0, 10		# r1 = 10
cmpneq p2 = r1, 10	# p2 = 1
pand p2 = p0, p1	# p2 = 0

# RegEx 2
lwc r10 = [r1 + 1]

# RegEx 3
swc [r1 + 1] = r2

# RegEx 4
mfs r8 = s6
mts s6 = r1

# RegEx 5
mul r1, r2

# RegEx 6
sres 4

# RegEx bcopy
bcopy r1 = r0, 5, ~p1

# Label and pred
label1: (!p1) add r1 = r2, 100


# Check special reg mapping
mts sl = r1
mts sh = r2
mts ss = r3
mts st = r4
mts srb = r5
mts sro = r6
mts sxb = r7
mts sxo = r8



# Check wrong des register
add p1 = r0, 10
cmpneq r2 = r1, 5
pand s2 = p0, p1
lwc p10 = [r1 + 1]
swc [r50 + 1] = r2
mfs s8 = s6
mts r6 = r1